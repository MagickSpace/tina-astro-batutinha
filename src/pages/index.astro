---
import Base from '../layouts/base.astro'
import Hero from '../components/hero.astro'
import CTAmatriculas from '../components/cta_matriculas.astro'

import { client } from '../../.tina/__generated__/client'
import { MyComponent } from '../components/react'
 
const response = await client.queries.post({relativePath: "home.md"}) 
 
 
---



<Base>

	<Hero/>
	<CTAmatriculas/>
	<main>
		<!-- No way for visual editing to work with .astro components -->
	<!-- <pre  style={{margin: '100px auto', width: '1000px'}}>{JSON.stringify(response.data.post.body, null, 2)}</pre> -->
		<!-- Uncomment React version to see Tina working -->
		<MyComponent client:tina {...response} myField={'title'}/>
	</main>
</Base>